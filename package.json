    "date-fns": "^3.6.0",
    "dayjs": "^1.11.13",
    "docx": "^8.5.0",
    "emailjs-com": "^3.2.0",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "firebase": "^10.12.4",
    "formik": "^2.4.5",
    "html2canvas": "^1.4.1",
