{
  "name": "prueba-ecommerce",
  "private": true,
  "version": "0.0.93",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"cd backend && npm run dev\"",
    "dev:android": "concurrently \"vite\" \"cd backend && npm run dev\" \"npm run android:dev\"",
    "dev:staging": "vite --mode staging",
    "dev:production": "vite --mode production",
    "build": "vite build",
    "build:staging": "vite build --mode staging",
    "build:production": "vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "setup:dev": "node scripts/setup-environments.js development",
    "setup:staging": "node scripts/setup-environments.js staging",
    "setup:production": "node scripts/setup-environments.js production",
    "backend:dev": "cd backend && npm run dev",
    "backend:start": "cd backend && npm start",
    "start:full": "concurrently \"npm run dev\" \"npm run backend:dev\"",
    "deploy:staging": "npm run build:staging && vercel --prod",
    "deploy:production": "npm run build:production && vercel --prod",
    "cap:add:android": "npx cap add android",
    "cap:add:ios": "npx cap add ios",
    "cap:sync": "npx cap sync",
    "cap:build": "npm run build && npx cap sync",
    "cap:open:android": "npx cap open android",
    "cap:open:ios": "npx cap open ios",
    "cap:run:android": "npx cap run android",
    "cap:run:ios": "npx cap run ios",
    "fer": "npm run build && cd android && gradlew clean && cd .. && npx cap sync && android\\auto-fix-java.bat && cd android && gradlew assembleDebug && cd ..",
    "fer:fast": "node scripts/build-apk-optimized.js",
    "die": "node scripts/release.js",
    "android:dev": "npm run build && npx cap sync && android\\auto-fix-java.bat && npx cap run android",
    "android:build": "npm run build && cd android && gradlew clean && cd .. && npx cap sync && android\\auto-fix-java.bat && cd android && gradlew assembleDebug && cd ..",
    "android:clean": "cd android && gradlew clean && cd ..",
    "dev:web": "vite",
    "dev:full": "concurrently \"npm run dev\" \"npm run backend:dev\"",
    "build:full": "npm run build && npx cap sync",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "clean": "rm -rf dist node_modules/.vite",
    "optimize": "npm run clean && npm run build",
    "setup:repo": "node scripts/setup-public-repo.js",
    "setup:google-oauth": "node scripts/setup-google-oauth.js",
    "setup:java": "scripts\\setup-java-env.bat"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.2",
    "@capacitor/app": "^7.0.2",
    "@capacitor/browser": "^7.0.2",
    "@capacitor/core": "^7.4.2",
    "@capacitor/ios": "^7.4.2",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@fontsource/roboto": "^5.0.5",
    "@fullcalendar/core": "^6.1.18",
    "@fullcalendar/daygrid": "^6.1.18",
    "@fullcalendar/interaction": "^6.1.18",
    "@fullcalendar/react": "^6.1.18",
    "@mercadopago/sdk-react": "^0.0.13",
    "@mui/icons-material": "^5.14.1",
    "@mui/material": "^5.16.6",
    "@mui/x-date-pickers": "^8.8.0",
    "@react-pdf/renderer": "^3.4.4",
    "@southdevs/capacitor-google-auth": "^7.0.0",
    "axios": "^1.10.0",
    "bootstrap": "^5.3.3",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.13",
    "docx": "^8.5.0",
    "emailjs-com": "^3.2.0",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "firebase": "^10.12.4",
    "formik": "^2.4.5",
    "html2canvas": "^1.4.1",
    "html2pdf.js": "^0.10.2",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.2",
    "notistack": "^3.0.2",
    "pdf-lib": "^1.17.1",
    "pdfkit": "^0.15.0",
    "pdfmake": "^0.2.10",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.4",
    "react-calendar": "^5.0.0",
    "react-dom": "^18.2.0",
    "react-notifications-component": "^4.0.1",
    "react-router-dom": "^6.14.2",
    "react-signature-canvas": "^1.0.7",
    "react-to-print": "^3.1.1",
    "react-toastify": "^10.0.4",
    "sweetalert2": "^11.7.20",
    "uuid": "^9.0.0",
    "xlsx": "^0.18.5",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "cssnano": "^7.0.6",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "terser": "^5.32.1",
    "typescript": "^5.9.2",
    "vite": "^4.5.14",
    "vite-bundle-analyzer": "^0.7.0"
  }
}
