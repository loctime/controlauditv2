{
  "name": "prueba-ecommerce",
  "private": true,
  "version": "0.0.24",
  "type": "module",
  "packageManager": "pnpm@10.18.3",
  "engines": {
    "node": ">=18.0.0 <21.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "dev": "concurrently \"vite\" \"cd backend && pnpm run dev\"",
    "dev:staging": "vite --mode staging",
    "dev:production": "vite --mode production",
    "build": "vite build",
    "build:staging": "vite build --mode staging",
    "build:production": "vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "setup:dev": "node scripts/setup-environments.js development",
    "setup:staging": "node scripts/setup-environments.js staging",
    "setup:production": "node scripts/setup-environments.js production",
    "backend:dev": "cd backend && pnpm run dev",
    "backend:start": "cd backend && pnpm start",
    "deploy:staging": "pnpm run build:staging && vercel --prod",
    "deploy:production": "pnpm run build:production && vercel --prod",
    "die": "node scripts/release.js",
    "dev:web": "vite",
    "reinstall": "powershell -Command \"Remove-Item -Recurse -Force node_modules -ErrorAction SilentlyContinue; Remove-Item -Force pnpm-lock.yaml -ErrorAction SilentlyContinue; pnpm install\"",
    "analyze": "pnpm run build && npx vite-bundle-analyzer dist",
    "clean": "rm -rf dist node_modules/.vite",
    "optimize": "pnpm run clean && pnpm run build"
  },
  "dependencies": {
    "@controlfile/feedback-sdk": "github:loctime/feedback-skd",
    "@controlfile/sdk": "github:loctime/controlfile-sdk#v1.0.8",
    "@emotion/cache": "^11.13.0",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@fontsource/roboto": "^5.0.5",
    "@fullcalendar/core": "^6.1.18",
    "@fullcalendar/daygrid": "^6.1.18",
    "@fullcalendar/interaction": "^6.1.18",
    "@fullcalendar/react": "^6.1.18",
    "@mercadopago/sdk-react": "^0.0.13",
    "@mui/icons-material": "^5.14.1",
    "@mui/material": "^5.16.6",
    "@mui/x-date-pickers": "^8.8.0",
    "@react-pdf/renderer": "^3.4.4",
    "@tanstack/react-query": "^5.90.16",
    "axios": "^1.10.0",
    "bootstrap": "^5.3.3",
    "chart.js": "^4.1.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.13",
    "docx": "^8.5.0",
    "emailjs-com": "^3.2.0",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "firebase": "^10.12.4",
    "formik": "^2.4.5",
    "html2canvas": "^1.4.1",
    "html2pdf.js": "^0.10.2",
    "idb": "^8.0.3",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.2",
    "notistack": "^3.0.2",
    "pdf-lib": "^1.17.1",
    "pdfkit": "^0.15.0",
    "pdfmake": "^0.2.10",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.4",
    "react-calendar": "^5.0.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-notifications-component": "^4.0.1",
    "react-router-dom": "^6.14.2",
    "react-signature-canvas": "^1.0.7",
    "react-to-print": "^3.1.1",
    "react-toastify": "^10.0.4",
    "recharts": "^2.15.4",
    "sweetalert2": "^11.7.20",
    "tailwind-merge": "^3.3.1",
    "uuid": "^9.0.0",
    "xlsx": "^0.18.5",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "cssnano": "^7.0.6",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "sharp": "^0.34.4",
    "terser": "^5.32.1",
    "typescript": "^5.9.2",
    "vite": "^4.5.14",
    "vite-bundle-analyzer": "^0.7.0"
  }
}
