# Variables de entorno para PRODUCCIÓN
# Copiar este archivo como .env en el servidor de producción

# API de ControlFile (producción)
VITE_CONTROLFILE_API_URL=https://api.controlfile.app

# Firebase (producción)
VITE_FIREBASE_API_KEY=tu_api_key_de_produccion
VITE_FIREBASE_AUTH_DOMAIN=tu_proyecto.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=tu_proyecto_id
VITE_FIREBASE_STORAGE_BUCKET=tu_proyecto.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=tu_app_id

# Configuración de la aplicación
VITE_APP_NAME=ControlAudit
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=production

# URLs de la aplicación
VITE_APP_URL=https://auditoria.controldoc.app
VITE_API_URL=https://api.controlfile.app

# Configuración de CORS (para el backend)
CORS_ORIGIN=https://auditoria.controldoc.app 