# Configuración para desarrollo local
NODE_ENV=development

# Firebase Configuration
VITE_FIREBASE_API_KEY=tu_api_key_aqui
VITE_FIREBASE_AUTH_DOMAIN=tu_proyecto.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=tu_proyecto_id
VITE_FIREBASE_STORAGE_BUCKET=tu_proyecto.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef
VITE_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# Backend Configuration
VITE_BACKEND_URL=http://localhost:4000

# Admin Codes
VITE_ADMIN_CODE=AUDITORIA2024
VITE_SUPER_ADMIN_CODE=SUPERMAX2024

# Debug Mode
VITE_DEBUG_MODE=true
VITE_ENABLE_LOGS=true

# ControlFile Integration (Auth compartido con ControlFile)
# Si no se configuran, usará las variables VITE_FIREBASE_* por defecto
VITE_CONTROLFILE_AUTH_PROJECT_ID=
VITE_CONTROLFILE_AUTH_API_KEY=
VITE_CONTROLFILE_AUTH_DOMAIN=
VITE_CONTROLFILE_BACKEND_URL=https://controlfile.onrender.com